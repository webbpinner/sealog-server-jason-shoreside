    "eventlogging"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "glue": "^5.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^17.8.1",
    "hapi-auth-jwt2": "^8.2.0",
    "hapi-mongodb": "^8.0.0",
    "hapi-swagger": "^9.3.0",
    "inert": "^5.1.2",
    "joi": "^14.3.1",
    "jquery-extend": "^2.0.3",
    "json-2-csv": "^3.1.1",
    "mongodb": "^3.1.10",
